pipeline {
    agent any
    environment {
    PATH= "$PATH:/opt/maven1/bin"
    }
    stages {
        stage ('Get code from git') {
            steps {
               git 'https://github.com/vaibhav7797/SaiJavaCode.git'
            }
        }
}
        stage ('Build') {
            steps {
                sh 'mvn clean package'
            }
       } 
}
